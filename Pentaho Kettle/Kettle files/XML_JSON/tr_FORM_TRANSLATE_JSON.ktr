    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var pairs &#x3d; to_translate.split&#x28;&#x22;&#x23;,&#x22;&#x29;&#x3b;&#xd;&#xa;var pairs_length &#x3d; pairs.length&#x3b;&#xd;&#xa;var number_of_jsons &#x3d; Math.ceil&#x28;pairs_length &#x2f; limit_of_segments&#x29;&#x3b;&#xd;&#xa;var json_strings &#x3d; &#x22;&#x22;&#x3b;&#xd;&#xa;var keys_count &#x3d; 0&#x3b;&#xd;&#xa;var used_keys &#x3d; &#x5b;&#x5d;&#x3b;&#xd;&#xa;var langs_split &#x3d; langs.split&#x28;&#x22;-&#x22;&#x29;&#x3b;&#xd;&#xa;var src_lang &#x3d; langs_split&#x5b;0&#x5d;&#x3b;&#xd;&#xa;var trg_lang &#x3d; langs_split&#x5b;1&#x5d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f; Generate the JSONs that will be sent to translate&#xd;&#xa;for &#x28;var i&#x3d;0&#x3b; i&#x3c;number_of_jsons&#x3b; i&#x2b;&#x2b;&#x29;&#x7b;&#xd;&#xa;	var pairs_num &#x3d; 0&#x3b;&#xd;&#xa;	if &#x28;pairs.length &#x3e;&#x3d; limit_of_segments&#x29;&#x7b;&#xd;&#xa;		pairs_num &#x3d; limit_of_segments&#x3b;&#xd;&#xa;	&#x7d; else &#x7b;&#xd;&#xa;		pairs_num &#x3d; pairs.length&#x3b;&#xd;&#xa;	&#x7d;&#xd;&#xa;	var current_pairs &#x3d; pairs.slice&#x28;0, pairs_num&#x29;&#xd;&#xa;	&#xd;&#xa;	var json_translate &#x3d; &#x22;&#x7b;&#x22;&#x3b;&#xd;&#xa;	json_translate &#x2b;&#x3d; &#x22;&#x5c;&#x22;token&#x5c;&#x22;&#x3a; &#x5c;&#x22;&#x22; &#x2b; iadaatpa_api_key &#x2b; &#x22;&#x5c;&#x22;, &#x22;&#x3b;&#xd;&#xa;	json_translate &#x2b;&#x3d; &#x22;&#x5c;&#x22;source&#x5c;&#x22;&#x3a; &#x22; &#x2b; &#x22;&#x5c;&#x22;&#x22; &#x2b; src_lang &#x2b; &#x22;&#x5c;&#x22;,&#x22;&#x3b;&#xd;&#xa;	json_translate &#x2b;&#x3d; &#x22;&#x5c;&#x22;target&#x5c;&#x22;&#x3a; &#x22; &#x2b; &#x22;&#x5c;&#x22;&#x22; &#x2b; trg_lang &#x2b; &#x22;&#x5c;&#x22;,&#x22;&#x3b;&#xd;&#xa;	json_translate &#x2b;&#x3d; &#x22;&#x5c;&#x22;segments&#x5c;&#x22;&#x3a; &#x7b;&#x22;&#x3b;&#xd;&#xa;	&#xd;&#xa;	for&#x28;var count &#x3d; 0&#x3b; count &#x3c; pairs_num&#x3b; count&#x2b;&#x2b;&#x29;&#x7b;&#xd;&#xa;		var pair &#x3d; current_pairs&#x5b;count&#x5d;&#x3b;&#xd;&#xa;		var elems &#x3d; pair.split&#x28;&#x22;&#x3b;&#x22;&#x29;&#x3b;&#xd;&#xa;		var key &#x3d; elems&#x5b;0&#x5d;&#x3b;&#xd;&#xa;		var val &#x3d; elems&#x5b;1&#x5d;&#x3b;&#xd;&#xa;		&#x2f;&#x2f; Check that the key has not bein used&#xd;&#xa;		if &#x28;&#x28;used_keys.indexOf&#x28;key&#x29; &#x3e; -1&#x29; &#x3d;&#x3d; false&#x29;&#x7b;&#xd;&#xa;			&#x2f;&#x2f; Add the key to the used keys&#xd;&#xa;			used_keys.push&#x28;key&#x29;&#x3b;&#xd;&#xa;		&#x7d; else &#x7b;&#xd;&#xa;			&#x2f;&#x2f; The ID of the key has to be unique in the REST call&#xd;&#xa;			key &#x2b;&#x3d; &#x22;---&#x22; &#x2b; keys_count&#x3b;&#xd;&#xa;			used_keys.push&#x28;key&#x29;&#x3b;&#xd;&#xa;			keys_count &#x2b;&#x3d; 1&#x3b;&#xd;&#xa;		&#x7d;&#xd;&#xa;		&#x2f;&#x2f; Add the key and the value to the segments dict&#xd;&#xa;		json_translate &#x2b;&#x3d; &#x22;&#x5c;&#x22;&#x22; &#x2b; key &#x2b; &#x22;&#x5c;&#x22;&#x3a; &#x22; &#x2b; &#x22;&#x5c;&#x22;&#x22; &#x2b; val &#x2b; &#x22;&#x5c;&#x22;,&#x22;&#x3b;&#xd;&#xa;	&#x7d;&#xd;&#xa;	&#x2f;&#x2f; Remove first n elements from the array of pairs&#xd;&#xa;	pairs.splice&#x28;0, pairs_num&#x29;&#x3b;&#xd;&#xa;	&#x2f;&#x2f; Remove all the used keys for this iteration&#xd;&#xa;	used_keys.slice&#x28;0, used_keys.length-1&#x29;&#x3b;&#xd;&#xa;	&#x2f;&#x2f; Close of segments and current JSON&#xd;&#xa;	if &#x28;json_translate.substr&#x28;json_translate.length - 1&#x29; &#x3d;&#x3d; &#x22;,&#x22;&#x29;&#x7b;&#xd;&#xa;		json_translate &#x3d; json_translate.substr&#x28;0, json_translate.length - 1&#x29;&#x3b;&#xd;&#xa;	&#x7d;&#xd;&#xa;	json_translate &#x2b;&#x3d; &#x22;&#x7d;&#x7d;&#x22;&#x3b;&#xd;&#xa;	&#x2f;&#x2f; Add current JSON to the final JSONs String&#xd;&#xa;	json_strings &#x2b;&#x3d; json_translate &#x2b; &#x22;&#x23;&#x22;&#x3b;&#xd;&#xa;&#x7d;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f; Delete the last appearence of the delimiter&#xd;&#xa;if &#x28;json_strings.substr&#x28;json_strings.length - 1&#x29; &#x3d;&#x3d; &#x22;&#x23;&#x22;&#x29;&#x7b;&#xd;&#xa;	json_strings &#x3d; json_strings.substr&#x28;0, json_strings.length - 1&#x29;&#x3b;&#xd;&#xa;&#x7d;</jsScript_script>
